ZSH_CONFIG_DIR=~/.config/zsh

source $ZSH_CONFIG_DIR/aliases
source $ZSH_CONFIG_DIR/keybindings
source $ZSH_CONFIG_DIR/path

source $ZSH_CONFIG_DIR/completions/init
source $ZSH_CONFIG_DIR/autosuggestions.zsh
source $ZSH_CONFIG_DIR/fsh/fast-syntax-highlighting.plugin.zsh

eval "$(starship init zsh)"
eval "$(zoxide init zsh)"
